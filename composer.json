{
    "name": "lancasters/tests-library",
    "description": "Tests library",
    "repositories": [
        {
		"type": "composer",
		"url": "https://repo.packagist.com/lancastersolutions/"
	}
    ],
    "require": {
        "symfony/monolog-bundle": "^3.1",
        "symfony/yaml": "^4.0|^5.0|^6.0",
        "psr/cache": "^1|^2|^3",
        "psr/log": "^1|^2|^3",
        "symfony/deprecation-contracts": "2.5.*",
        "symfony/var-exporter": "^5.0|^6.0",
        "lancasters/curl-library": "2.*",
        "assuncaofranco/test-library": "^1.0"
    },
    "autoload": {
        "psr-4": { "": "src/" }
    },
    "autoload-dev": {
        "psr-4": { "Tests\\": "tests/" }
    },
    "license": "Proprietary"
}
