{
    "name": "lancasters/curl-library",
    "description": "Library for uniformization of requests between microservices",
    "license": "Proprietary",
    "autoload": {
        "psr-4": {
            "": "src/"
        }
    },
    "repositories": [
        {"type": "composer", "url": "https://repo.packagist.com/lancastersolutions/"}
    ],
    "require": {
        "php": ">=7.4",
        "ext-json": "*",
        "blackfire/php-sdk": "^1.17",
        "guzzlehttp/guzzle": "^6.1|^7.3",
        "monolog/monolog": "^1.16|^2.0|^3.0",
        "nyholm/psr7": "^1.7",
        "symfony/http-client": "^4.0|^5.0|^6.0",
        "symfony/http-foundation": "^4.0|^5.0|^6.0",
        "symfony/psr-http-message-bridge": "^2.0|^6.0"
    },
    "require-dev": {
        "squizlabs/php_codesniffer": "^2.7"
    },
    "config": {
        "sort-packages": true
    }
}
